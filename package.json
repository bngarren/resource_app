{
   "name": "resource-app",
   "private": true,
   "version": "0.0.2",
   "engines": {
      "node": "16.x"
   },
   "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
      "build": "tsc",
      "heroku-postbuild": "npm run migrate-latest",
      "deploy": "git push heroku master",
      "start": "node dist/index.js",
      "migrate-latest": "knex migrate:latest --knexfile src/knexfile.js",
      "migrate-rollback": "knex migrate:rollback --all --knexfile src/knexfile.js",
      "seed": "knex seed:run --knexfile src/knexfile.js"
   },
   "dependencies": {
      "body-parser": "^1.20.0",
      "cors": "^2.8.5",
      "dotenv": "^16.0.1",
      "express": "^4.18.1",
      "h3-js": "^3.7.2",
      "knex": "^2.1.0",
      "pg": "^8.7.3"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.2",
      "@types/cors": "^2.8.12",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.13",
      "@types/node": "^17.0.44",
      "@types/pg": "^8.6.5",
      "@typescript-eslint/eslint-plugin": "^5.19.0",
      "@typescript-eslint/parser": "^5.19.0",
      "eslint": "^8.13.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-prettier": "^4.0.0",
      "eslint-plugin-react": "^7.29.4",
      "eslint-plugin-react-hooks": "^4.4.0",
      "prettier": "^2.6.2",
      "ts-node-dev": "^2.0.0",
      "typescript": "^4.5.4"
   }
}
